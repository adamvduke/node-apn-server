form(name="input", action="/upload", method="post", id="input", enctype="multipart/form-data")
  div.row
    div.six.columns
      label App Id:
      input(type="text", name="appId")
    div.six.columns
      label App Secret:
      input(type="text", name="appSecret")
    label Certificate:
    input(type="file", name="certificate")
    label Key:
    input(type="file", name="key")
  div.row
    input(type="submit", value="Submit")

a(href="/") Send a notification!